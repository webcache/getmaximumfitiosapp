git_url("https://github.com/webcache/githubactionsstore.git")

storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.getmaximumfreedomandfitness.getmaximumfitiosapp"])

# Remove username to allow App Store Connect API authentication via SPACESHIP environment variables
# But provide username as fallback to prevent credentials manager from failing
username("kccashes@icloud.com") # Your Apple Developer Portal username from Appfile

# Only use ci-keys.keychain in CI environment, not locally
if ENV["CI"] || ENV["GITHUB_ACTIONS"]
  keychain_name("ci-keys.keychain")
end

# Use basic auth with token for GitHub access if available, otherwise use git_url with embedded token
if ENV["MATCH_GIT_BASIC_AUTHORIZATION"]
  git_basic_authorization(ENV["MATCH_GIT_BASIC_AUTHORIZATION"])
end

# For all available options run `fastlane match --help`
# Remove the # in the beginning of the line to enable the other options

# The docs are available on https://docs.fastlane.tools/actions/match
