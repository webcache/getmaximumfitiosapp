// Xcode Cloud Build Configuration
// This file contains build settings to fix common Xcode Cloud issues

// Disable problematic features that can cause target issues
ENABLE_STRICT_OBJC_MSGSEND = NO
ENABLE_USER_SCRIPT_SANDBOXING = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) EXPO_CONFIGURATION_RELEASE=1

// Code signing settings for Xcode Cloud
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = GKZM3F9S4U

// Build settings for stability
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
SWIFT_VERSION = 5.0

// Privacy manifest settings
GENERATE_INFOPLIST_FILE = YES
INFOPLIST_KEY_NSPrivacyCollectedDataTypes = NSPrivacyCollectedDataTypeEmailAddress
INFOPLIST_KEY_NSPrivacyTracking = NO
