{
  "expo": {
    "name": "GetMaximumFit",
    "slug": "getmaximumfitiosapp",
    "owner": "getmaximumfreedomandfitness",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "jsc",
    "newArchEnabled": false,
    "scheme": "GetMaximumFit",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getmaximumfreedomandfitness.getmaximumfitiosapp",
      "buildNumber": "99",
      "scheme": "GetMaximumFit",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/MF-logo-clean.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "google",
          "getmaximumfitiosapp",
          "fb",
          "instagram",
          "instagram-stories",
          "facebook-stories",
          "twitter",
          "whatsapp",
          "mailto"
        ],
        "GIDClientID": "424072992557-1iehcohe1bkudsr6qk4r85u13t9loa5o.apps.googleusercontent.com",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "Google Sign-In",
            "CFBundleURLSchemes": [
              "getmaximumfitiosapp"
            ]
          }
        ],
        "NSCameraUsageDescription": "We use your camera to take progress photos.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to customize your dashboard image.",
        "NSPhotoLibraryAddUsageDescription": "We need access to save selected images to your photo library for dashboard customization.",
        "PHPhotoLibraryPreventAutomaticLimitedAccessAlert": true,
        "NSLocationWhenInUseUsageDescription": "We use your location to customize your workout data.",
        "NSHealthShareUsageDescription": "This app requires access to HealthKit to track your fitness data.",
        "NSHealthUpdateUsageDescription": "This app updates your HealthKit data with workout sessions.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "accounts.google.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSExceptionRequiresForwardSecrecy": false
            },
            "googleapis.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSExceptionRequiresForwardSecrecy": false
            }
          }
        }
      },
      "entitlements": {
        "com.apple.developer.healthkit": true
      },
      "appleTeamId": "4H5UK5L4A8"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.getmaximumfreedomandfitness.getmaximumfitiosapp"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "./plugins/withGoogleServiceInfo",
      [
        "expo-build-properties",
        {
          "ios": {
            "ios.deploymentTarget": "15.1",
            "unstable_networkInspector": false,
            "ccacheEnabled": false,
            "newArchEnabled": false
          },
          "android": {
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you customize your dashboard image.",
          "cameraPermission": "The app accesses your camera to let you take progress photos.",
          "microphonePermission": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/MF-logo-clean.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "hideAsync": false
        }
      ],
      "expo-font",
      [
        "react-native-health",
        {
          "healthShare": "This app requires access to HealthKit to track your fitness data.",
          "healthUpdate": "This app updates your HealthKit data with workout sessions."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0865081a-dbbc-474a-9c9a-25279fadf91c"
      }
    }
  }
}
